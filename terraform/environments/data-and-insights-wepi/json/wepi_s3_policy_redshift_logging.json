{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Sid": "Put bucket policy needed for audit logging",
          "Effect": "Allow",
          "Principal": {
              "AWS": "arn:aws:iam::307160386991:user/logs"
          },
          "Action": [
            "s3:GetBucketAcl",
            "s3:PutObject"
          ],
          "Resource": [
              "${bucket_arn}",
              "${bucket_arn}/*"
          ]
      }
  ]
}